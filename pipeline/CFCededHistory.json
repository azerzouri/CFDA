					}
				],
				"outputs": [
					{
						"referenceName": "CFDMCededHistory",
						"type": "DatasetReference",
						"parameters": {
							"TableName": {
								"value": "@pipeline().parameters.TableName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"Partition": {
				"type": "string",
				"defaultValue": "2020-09-22"
			},
			"TableName": {
				"type": "string",
				"defaultValue": "CFADATANew"
			}
		},
		"annotations": []
	}
