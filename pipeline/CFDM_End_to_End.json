{
	"name": "CFDM_End_to_End",
	"properties": {
		"description": "this is the one and only end to pipeline to load all of cashflow dims and facts",
		"activities": [
			{
				"name": "Execute_cf_staging",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "OnStart_SendMail",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "cf_staging",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "Execute_cfdm_load",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "Execute_cf_staging",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "cfdm_load",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true
				}
			},
			{
				"name": "OnStart_SendMail",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://prod-42.eastus2.logic.azure.com:443/workflows/e1a498e592c74ccfa38822975735cb8f/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=BeS6_l3JEkWqHrQKwmGtZi4T0CVHw4nDW_X2OcVDrXw",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"value": "{\n   \"DataFactoryName\": \"@{pipeline().DataFactory}\",\n   \"PipelineName\": \"@{pipeline().Pipeline}\",\n   \"Subject\": \"The CFDM_END_TO_END pipeline started!\",\n   \"Message\": \"The CFDM_END_TO_END pipeline started!\",\n   \n}",
						"type": "Expression"
					}
				}
			}
		],
		"annotations": []
	}
}