										"Schema": {
											"value": "@item().TABLE_CATALOG",
											"type": "Expression"
										},
										"StoredProc": {
											"value": "@item().SQLtxt",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "CFDMCededHistory",
									"type": "DatasetReference",
									"parameters": {
										"TableName": {
											"value": "@{item().Table_Name}",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"variables": {
			"Copydata": {
				"type": "String",
				"defaultValue": "Select * from wrk.pipeline_parameter"
