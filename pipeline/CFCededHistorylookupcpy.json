						"value": "@equals(pipeline().DataFactory,'e2nprddfcf')",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "setcopydatatoFalse",
							"type": "SetVariable",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"variableName": "Copydata",
								"value": "Select * from wrk.pipeline_parameter where year(InsertedDate) = 2019"
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "Setcopydatatotrue",
							"type": "SetVariable",
							"dependsOn": [],
