{
	"name": "staging_Reinusrance",
	"properties": {
		"activities": [
			{
				"name": "Reinsurance_staging",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": "USE RS_ODS\n\nSELECT \n    F.ClaimNo, \n       F.ExposureNo, \n       F.SrcReinsur_ID as Sapiens_Reinsurer_ID, \n       F.SrcBrokerID as Sapiens_Broker_ID, \n       F.SrcPoolID as Sapiens_Pool_ID, \n\t   F.SecontRefNo, \n       F.RetentionInd,\n\t   convert(date ,DATEADD(dd, -1, DATEADD(qq, DATEDIFF(qq, 0, F.Valuation_Dt), 0))) as Entrydate,\n\t   C.Credit_Provision_Percent,\n       R.Reco_Desc as ReinsurerName, \n\n       \n\t   E.Pool_Name,\n\t   d.Broker_Desc,\n\t   b.Related_Claim_No,\nsum(F.Case_Loss) as projectedCededTotalPaidExInLimits ,    --Map to projectedCededTotalPaidExInLimits\nsum(F.Case_Adjusting_Expense) as projectedCededTotalPaidDefExpense, --Map to projectedCededTotalPaidDefExpense\nSum(F.Case_Coverage_DJ_Expense) as projectedCededPaidCoverageDjExpense --Map to projectedCededPaidCoverageDjExpense\n\n\nFROM [RS_ODS].[trn].[Ceded_Financial_Transaction] F\nleft JOIN RS_ODS.rein.Reinsurer R ON F.SrcReinsur_ID = R.System_Reco_Cd AND R.Sys_RowEndDt = '9999-12-31'\nleft join [rein].[Pool] as e on F.[PoolID] = e.[Pool_ID]\t\nleft join [rein].[Broker] as d on F.[BrokerID] = d.[BrokerID]\t\nleft join [ref].[Rein_Related_Claims] as b on F.[ClaimNo] = b.sapiens_Claim_no and F.[ExposureNo] = b.exposure_no\t\n\n\nLEFT JOIN RS_ODS.rein.Reinsurer_Credit_Provision C ON C.Reinsurer_ID = R.Reinsurer_ID AND C.Status_Cd = 'VAL' AND C.Sys_RowEndDt = '9999-12-31'\nWHERE Valuation_Dt =\n(SELECT MAX(Valuation_Dt)\n  FROM [RS_ODS].[trn].[Ceded_Financial_Transaction])\ngroup by\n  F.ClaimNo, \n       F.ExposureNo, \n       F.SrcReinsur_ID, \n       F.SrcBrokerID, \n       F.SrcPoolID, \n\t   F.SecontRefNo, \n       F.RetentionInd,\n\t   F.Valuation_Dt,\n       R.Reco_Desc, \n    C.Credit_Provision_Percent,\n   \n   E.Pool_Name,\n\t d.Broker_Desc\n\t ,b.Related_Claim_no\n\n\t   \n\t   \n\t  ",
						"queryTimeout": "02:00:00"
					},
					"sink": {
						"type": "SqlMISink"
					},
					"enableStaging": false,
